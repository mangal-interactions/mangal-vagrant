{
  "name": "mangal-backend",
  "version": "0.0.1",
  "description": "Backend server with API services for the mangal.io project",
  "main": "index.js",
  "scripts": {
    "deploy_dbs": "createdb -U postgres  mangal_dev; psql -U postgres -d mangal_dev -c 'CREATE EXTENSION postgis'; createdb -U postgres mangal_test; psql -U postgres -d mangal_test -c 'CREATE EXTENSION postgis';",
    "drop_dbs": "dropdb mangal_dev -U postgres --if-exists; dropdb mangal_test -U postgres  --if-exists;",
    "test": "NODE_ENV=test node_modules/.bin/mocha",
    "start": "NODE_ENV=development nodemon",
    "docco": "./node_modules/.bin/docco -o docs/pages index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mangal-wg/mangal-backend.git"
  },
  "keywords": [
    "interactions",
    "networks",
    "species",
    "ecology"
  ],
  "author": "Steve Vissault & Timoth√©e Poisot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mangal-wg/mangal-backend/issues"
  },
  "homepage": "https://github.com/mangal-wg/mangal-backend#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "docco": "^0.7.0",
    "epilogue": "^0.7.0",
    "express": "^4.14.0",
    "http": "0.0.0",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.24.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.0",
    "nodemon": "^1.10.2",
    "sequelize-cli": "^2.4.0",
    "supertest": "^2.0.0"
  }
}
